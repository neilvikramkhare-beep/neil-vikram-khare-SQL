INSERT INTO emp VALUES (4, 'Adarsh', 'Nashik', 9853654151, 3471, 'BP82', 'DBA', 1987, 'DBA', 48000);
INSERT INTO emp VALUES (5, 'Rohit', 'Kolkata', 7134823171, 8374, 'PG86', 'Finance', 1971, 'Finance', 39000);
INSERT INTO emp VALUES (6, 'Sania', 'Mysore', 8721387415, 4265, 'BM32', 'Accountant', 1965, 'Accountant', 41000);
SELECT count(emp_no), count(emp_name), count(salary)
FROM emp
GROUP BY designation
ORDER BY designation ASC;
SELECT count(salary)
FROM emp
WHERE salary > 48000;
SELECT emp_no, salary
FROM emp
WHERE salary > 48000
ORDER BY salary ASC, emp_no ASC;
SELECT count(*)
FROM emp
WHERE year > 1981
GROUP BY designation
ORDER BY designation ASC;
