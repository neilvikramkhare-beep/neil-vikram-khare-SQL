CREATE TABLE emp (emp_no INT,emp_name VARCHAR(255),emp_address VARCHAR(255),emp_phno BIGINT,dept_no INT,dept_name VARCHAR(255),job_id VARCHAR(255),year INT,designation VARCHAR(255),salary INT);
INSERT INTO emp VALUES (1, 'Pavan', 'Pune', 8876428111, 3871, 'AB12', 'Analyst', 1972, 'Analyst', 45000);
