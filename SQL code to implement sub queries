insert into emp(2345612675,Kriti,Hyderabad,5556565789,3478,analyst,1982,6200);
select*from emp where sal>(select sal from emp where dname="IT");
select enumber,ename,designation from emp where doj>1981 order by designation desc;
select enumber,ename,current date doj as exp, sal/30 as daily sal from emp;
select*from emp where(designation="clerk") or (designation="analyst");
