create table emp(enumber int(10),ename,address varchar(20),phone number int,dept number int,deptname char(20), job_id int,designation char(10),doj,salary int);
insert into emp(4626477230,Rahul,Pune,2532336252,5563, account,43454,account manager,1965,23000);
