CREATE TABLE emp ( emp_no INT PRIMARY KEY, emp_name VARCHAR(255),emp_address VARCHAR(255),emp_phno BIGINT,dept_no INT,dep_location VARCHAR(255),job_id INT,designation VARCHAR(255),salary INT,hire_date DATE);
