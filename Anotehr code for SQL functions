SELECT emp_name, hire_date FROM emp WHERE hire_date IN ('1981-05-01', '1981-12-03', '1981-12-17', '1980-01-19');
SELECT emp_name, dept_no FROM emp WHERE dept_no IN (10, 20);
SELECT emp_no, emp_name FROM emp WHERE emp_name LIKE 'S%';
SELECT emp_no, emp_name FROM emp WHERE emp_name LIKE 'H%';
SELECT emp_no, designation FROM emp WHERE designation = 'VP';
SELECT emp_no, emp_name FROM emp;
