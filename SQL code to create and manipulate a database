SHOW DATABASES;
USE megha;
CREATE TABLE stul (sid INT PRIMARY KEY,name CHAR(10) NOT NULL,addr VARCHAR(20) NOT NULL,fees INT CONSTRAINT chk CHECK (fees > 50000), email_id VARCHAR(20));
DESC stul;
ALTER TABLE stul ADD CONSTRAINT tuk UNIQUE (email_id);
DESC stul;
